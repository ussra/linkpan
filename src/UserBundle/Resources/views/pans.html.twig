{% block scripts %}
    {{ parent() }}
    <script src="{{ asset('Userscripts/js/wizard.js') }}"></script>

    <!-- remove pans-->
    <script>
        function removePans()
        {
            var selected = new Array();
            $('#datatable input[type="checkbox"]:checked').each(function() {
                selected.push($(this).attr('value'));
            });
            if(selected.length > 0)
            {
                $.ajax({
                    url:'{{ (path('remove_pan')) }}',
                    type: 'POST',
                    dataType: 'json',
                    data:
                        {
                            'selected':selected
                        },
                    async: true,
                    success: function (data)
                    {
                        alert('Deleted !')
                        location.reload();
                    }
                });
            }
            else
                alert("{{'please , choose the item to delete'|trans}}")
        }
    </script>
{% endblock %}