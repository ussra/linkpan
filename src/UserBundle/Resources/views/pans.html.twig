{% extends "UserBundle::crud.html.twig" %}


{% block style %}
    {{ parent() }}
    <link rel="stylesheet" href="{{ asset('Userscripts/css/wizard.css') }}">
{% endblock %}

{% block Title %}
    Pans
{% endblock %}


{% block thead %}
    <th></th>
    <th>{{'Name'|trans}}</th>
    <th>{{'Origine'|trans}}</th>
    <th>{{'Availability'|trans}}</th>
    <th>{{'Quantity'|trans}}</th>
    <th>{{'Category'|trans}}</th>
    <th>{{'Description'|trans}}</th>
    <th>{{'Created_On'|trans}}</th>
    <th width="5%"></th>
{% endblock %}

{% block tbody %}
    {% set pans =  app.session.get('boost_pans') %}

    {% for pan in pans %}
        <tr>
            <td width="30px;">
                <!-- <input type="checkbox" id="{ pan.pan_id }}" class="flat" name="CheckedPoste" value="{ pan.pan_id }}"> -->
                <!-- Default checkbox -->
                <div class="checkbox">
                    <label>
                    <input type="checkbox" value="{{ pan.pan_id }}">
                    <span class="cr"><i class="cr-icon glyphicon glyphicon-ok"></i></span>
                    </label>
                </div>
            </td>
            <td>{{ pan.pan_name }}</td>
            <td>{{ pan.pan_origine }}</td>
            <td>{{ pan.pan_av }}</td>
            <td>{{ pan.pan_quantity }}</td>
            <td>{{ pan.pan_category }}</td>
            <td>{{ pan.pan_description }}</td>
            <td>{{ pan.pan_creation_date }}</td>
            <td>
                {% if pan.pan_boost_type == 'Remove Boost' %}
                    <button type="submit" formmethod="post" formaction="{{ path('remove_boost_pan',{'pan':pan.pan_id  }) }}"  class="btn btn-danger btn-xs removeBoost"  title="remove boosting"> remove boosting </button>
                    {% else %}
                        <button type="submit" formmethod="post" formaction="{{ path('boost_pan',{'pan':pan.pan_id  }) }}" class="btn btn-default btn-xs boost"  title="Boost" > Boost </button>
                {% endif %}
            </td>
        </tr>
    {% endfor %}
{% endblock %}

{% block deleteaction %}removePans(){% endblock %}


